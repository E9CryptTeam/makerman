<template id="signal-template">
  {{#data}}
      <tr>
          <td>{{id}}</td>
          <td>{{symbol}}</td>
          <td>{{route}}</td>
          <td>{{percentage}}</td>
      </tr>
  {{/data}}
</template>

<table
  hx-ext="sse"
  sse-connect="signals/hx-stream">
  <thead>
      <tr>
          <th scope="col">Symbol</th>
          <th scope="col">Route</th>
          <th scope="col">Percentage</th>
          <th scope="col">Buy</th>
          <th scope="col">Price</th>
      </tr>
  </thead>
  <tbody
    sse-swap="SIGNAL"
  >
  </tbody>
</table>

